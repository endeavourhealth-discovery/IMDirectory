{
  "name": "im-ui",
  "version": "2.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run copy-themes && run-p dev:tsc dev:vite",
    "dev:tsc": "vue-tsc --noEmit --watch",
    "tsc": "vue-tsc --noEmit",
    "copy-themes": "copyfiles -u 3 ./node_modules/primevue/resources/themes/**/* ./public/",
    "dev:vite": "vite --port 8082",
    "dev:mocked": "vite --mode mock  --port 8082 ",
    "preview": "vite preview --port 8082",
    "build": "npm run copy-themes && vue-tsc --noEmit && vite build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:run": "cypress run",
    "test:e2e": "cypress open --browser chrome",
    "test:e2e:ci": "start-server-and-test preview http://localhost:8082 cypress:run",
    "lint": "eslint --fix",
    "format": "prettier --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-brands-svg-icons": "6.3.0",
    "@fortawesome/free-regular-svg-icons": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@fortawesome/vue-fontawesome": "3.0.3",
    "@sass-collective/strip-unit": "3.1.2",
    "abortcontroller-polyfill": "1.7.5",
    "aws-amplify": "5.0.8",
    "axios": "1.2.2",
    "chart.js": "4.1.1",
    "copyfiles": "2.4.1",
    "core-js": "3.27.1",
    "d3": "7.8.0",
    "google-palette": "1.1.0",
    "json5": "2.2.3",
    "lodash": "4.17.21",
    "path": "0.12.7",
    "primeflex": "3.3.0",
    "primeicons": "6.0.1",
    "primevue": "3.24.0",
    "sass": "1.57.1",
    "svg-pan-zoom": "3.6.1",
    "sweetalert2": "11.4.18",
    "tslib": "2.4.1",
    "uuid-random": "1.3.2",
    "vue": "3.2.45",
    "vue-json-pretty": "2.2.3",
    "vue-router": "4.1.6",
    "vue3-clipboard": "1.0.0",
    "vuedraggable": "4.1.0",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "5.0.2",
    "@cypress/vue": "5.0.3",
    "@faker-js/faker": "7.6.0",
    "@mswjs/data": "0.11.0",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@testing-library/vue": "6.6.1",
    "@types/d3": "7.4.0",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitest/coverage-c8": "0.26.3",
    "@vue/compiler-dom": "3.2.45",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.2.7",
    "cross-env": "7.0.3",
    "cypress": "12.3.0",
    "eslint": "8.31.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.8.0",
    "headers-polyfill": "3.1.2",
    "jsdom": "20.0.3",
    "msw": "0.49.2",
    "prettier": "2.8.1",
    "start-server-and-test": "1.15.2",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-conditional-compiler": "^0.1.1",
    "vitest": "0.26.3",
    "vue-tsc": "^1.2.0"
  },
  "optionalDependencies": {
    "@fortawesome/fontawesome-pro": "6.3.0",
    "@fortawesome/pro-duotone-svg-icons": "6.3.0",
    "@fortawesome/pro-light-svg-icons": "6.3.0",
    "@fortawesome/pro-regular-svg-icons": "6.3.0",
    "@fortawesome/pro-solid-svg-icons": "6.3.0",
    "@fortawesome/pro-thin-svg-icons": "6.3.0",
    "@fortawesome/sharp-regular-svg-icons": "6.3.0",
