{
  "name": "im-directory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "copy package-lock.json.clean package-lock.json || cp package-lock.json.clean package-lock.json",
    "dev": "npm link im-library && vite --port 8082",
    "preview": "vite preview --port 8082",
    "build": "vite build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage && c8 --reporter=lcov report",
    "cypress:run": "cypress run",
    "test:e2e": "cypress open --browser chrome",
    "test:e2e:ci": "start-server-and-test preview http://localhost:8082 cypress:run",
    "lint": "eslint --fix",
    "format": "prettier --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "aws-amplify": "^4.3.14",
    "axios": "^0.21.4",
    "chart.js": "^3.7.0",
    "core-js": "^3.21.0",
    "google-palette": "^1.1.0",
    "im-library": "https://endeavour-codebuild-output.s3.eu-west-2.amazonaws.com/im-library-1.0.0.tgz",
    "path": "^0.12.7",
    "primeflex": "^3.1.2",
    "primeicons": "^5.0.0",
    "primevue": "^3.11.1",
    "sass": "^1.49.9",
    "tslib": "^2.3.1",
    "vue": "^3.2.30",
    "vue-router": "^4.0.12",
    "vue3-clipboard": "^1.0.0",
    "vuex": "^4.0.1"
