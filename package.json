{
  "name": "im-ui",
  "version": "2.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p tsc:dev dev:vite",
    "tsc:dev": "vue-tsc --noEmit --watch",
    "tsc:build": "vue-tsc --noEmit",
    "dev:vite": "vite --port 8082",
    "dev:mocked": "vite --mode mock  --port 8082 ",
    "preview": "vite preview --port 8082",
    "build": "pnpm tsc:build && vite build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:run": "cypress run",
    "test:e2e": "cypress open --browser chrome",
    "test:e2e:ci": "pnpm clean-e2e && pnpm stop-only && cypress run --browser chrome --headless --reporter junit --reporter-options \"mochaFile=cypress/e2e-results.[hash].xml\"",
    "lint": "eslint --fix",
    "format": "prettier --write",
    "clean-install": "rimraf pnpm-lock.yaml && rimraf node_modules && pnpm install",
    "clean-e2e": "rimraf ./cypress/downloads && rimraf ./cypress/screenshots && rimraf --glob ./cypress/e2e-results.*.xml",
    "stop-only": "node stop-only.js"
  },
  "dependencies": {
    "@braintree/sanitize-url": "7.1.1",
    "@primeuix/themes": "^1.0.0",
    "@sass-collective/strip-unit": "3.1.2",
    "@vee-validate/yup": "4.15.0",
    "abortcontroller-polyfill": "1.7.8",
    "aws-amplify": "6.13.2",
    "axios": "1.8.1",
    "chart.js": "4.4.8",
    "copyfiles": "2.4.1",
    "core-js": "3.41.0",
    "d3": "7.9.0",
    "dotenv": "16.4.7",
    "glob": "11.0.1",
    "google-palette": "1.1.1",
    "ip": "2.0.1",
    "json5": "2.2.3",
    "lodash-es": "4.17.21",
    "pinia": "3.0.1",
    "primeicons": "7.0.0",
    "primevue": "4.3.1",
    "qr-code-styling": "1.9.1",
    "sass": "1.85.1",
    "semver": "7.7.1",
    "svg-pan-zoom": "3.6.2",
    "sweetalert2": "11.17.2",
    "tslib": "2.8.1",
    "typescript-logging": "2.2.0",
    "typescript-logging-log4ts-style": "2.2.0",
    "uuid": "11.1.0",
    "vee-validate": "4.15.0",
    "vue": "3.5.13",
    "vue-json-pretty": "2.4.0",
    "vue-router": "4.5.0",
    "vue-showdown": "4.2.0",
    "vue3-clipboard": "1.0.0",
    "vuedraggable": "2.24.3",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@faker-js/faker": "9.5.1",
    "@mswjs/data": "0.16.2",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@pinia/testing": "1.0.0",
    "@tailwindcss/vite": "^4.0.9",
    "@testing-library/vue": "8.1.0",
    "@types/d3": "7.4.3",
    "@types/jsonpath": "0.2.4",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.13.9",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-v8": "^3.0.7",
    "@vue/compiler-dom": "3.5.13",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.4.0",
    "@vue/test-utils": "2.4.6",
    "cross-env": "7.0.3",
    "cypress": "14.1.0",
    "eslint": "9.21.0",
    "eslint-plugin-cypress": "4.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "9.32.0",
    "functions-have-names": "1.2.3",
    "happy-dom": "17.1.9",
    "headers-polyfill": "4.0.3",
    "msw": "2.7.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "rimraf": "6.0.1",
    "tailwindcss": "4.0.9",
    "tailwindcss-primeui": "0.5.1",
    "typescript": "5.8.2",
    "vite": "6.2.0",
    "vitest": "3.0.7",
    "vue-tsc": "2.2.8"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "types": "./types",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@badeball/cypress-cucumber-preprocessor",
      "core-js",
      "cypress",
      "esbuild",
      "msw",
      "vue-demi"
    ],
    "onlyBuiltDependencies": [
      "@badeball/cypress-cucumber-preprocessor",
      "core-js",
      "cypress",
      "esbuild",
      "msw",
      "vue-demi"
    ]
  }
}
