{
  "name": "im-library",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clear": "rimraf dist/*",
    "build": "clear && tsc && rollup --config",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --fix",
    "format": "prettier --write"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.0",
    "google-palette": "1.1.0",
    "typescript": "4.7.4",
    "vitest": "0.15.1"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/d3": "^7.4.0",
    "d3": "^7.7.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "rollup": "^3.6.0",
    "rollup-plugin-dts": "^5.0.0",
    "uuid-random": "^1.3.2"
  },
  "main": "./dist/im-library.js",
  "typings": "./dist/im-library.d.ts",
  "module": "dist",
  "exports": {
    ".": {
      "import": "./dist/im-library.js",
      "types": "./dist/im-library.d.ts"
    },
    "./config/": {
      "import": "./dist/packages/im-library/src/config/modules/",
      "types": "./dist/types/config/"
    },
    "./config": {
      "import": "./dist/packages/im-library/src/config/index.js",
      "types": "./dist/types/config/index.d.ts"
    },
    "./constants/": {
      "import": "./dist/packages/im-library/src/constants/modules/",
      "types": "./dist/types/constants/"
    },
    "./constants": {
      "import": "./dist/packages/im-library/src/constants/index.js",
      "types": "./dist/types/constants/index.d.ts"
    },
    "./enums/": {
      "import": "./dist/packages/im-library/src/enums/modules/",
      "types": "./dist/types/enums/"
    },
    "./enums": {
      "import": "./dist/packages/im-library/src/enums/index.js",
      "types": "./dist/types/enums/index.d.ts"
    },
    "./helpers/": {
      "import": "./dist/packages/im-library/src/helpers/modules/",
      "types": "./dist/types/helpers/"
    },
    "./helpers": {
      "import": "./dist/packages/im-library/src/helpers/index.js",
      "types": "./dist/types/helpers/index.d.ts"
    },
    "./interfaces": "./dist/types/interfaces/index.d.ts",
    "./models/": {
      "import": "./dist/packages/im-library/src/models/modules/",
      "types": "./dist/types/models/"
    },
    "./models": {
      "import": "./dist/packages/im-library/src/models/index.js",
      "types": "./dist/types/models/index.d.ts"
    },
    "./vocabulary/": {
      "import": "./dist/packages/im-library/src/vocabulary/modules/",
      "types": "./dist/types/vocabulary/"
    },
    "./vocabulary": {
      "import": "./dist/packages/im-library/src/vocabulary/index.js",
      "types": "./dist/types/vocabulary/index.d.ts"
    }
  }
}
