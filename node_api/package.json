    "nodemon": "set NODE_ENV=production && nodemon dist/server",
    "build": "rimraf dist && pnpm swagger-autogen && pnpm tsc:build && vite build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
