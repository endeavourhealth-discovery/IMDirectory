    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@fortawesome/vue-fontawesome": "3.0.3",
    "@sass-collective/strip-unit": "3.1.2",
    "abortcontroller-polyfill": "1.7.5",
    "aws-amplify": "5.3.13",
    "axios": "1.6.2",
    "chart.js": "4.3.0",
    "copyfiles": "2.4.1",
    "core-js": "3.31.0",
    "d3": "7.8.5",
    "google-palette": "1.1.1",
    "json5": "2.2.3",
    "lodash": "4.17.21",
    "path": "0.12.7",
    "pinia": "2.1.4",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primevue": "3.35.0",
    "qr-code-styling": "1.6.0-rc.1",
    "sass": "1.68.0",
    "semver": "7.5.4",
    "svg-pan-zoom": "3.6.1",
    "sweetalert2": "11.7.2",
    "tslib": "2.6.2",
