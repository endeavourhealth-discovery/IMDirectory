{
  "name": "im-ui",
  "version": "2.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p tsc:dev dev:vite",
    "tsc:dev": "vue-tsc --noEmit --watch",
    "tsc:build": "vue-tsc --noEmit",
    "dev:vite": "vite --port 8082",
    "dev:mocked": "vite --mode mock  --port 8082 ",
    "preview": "vite preview --port 8082",
    "build": "pnpm tsc:build && vite build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:run": "cypress run",
    "test:e2e": "cypress open --browser chrome",
    "test:e2e:ci": "pnpm clean-e2e && pnpm stop-only && cypress run --browser chrome --headless --reporter junit --reporter-options \"mochaFile=cypress/e2e-results.[hash].xml\"",
    "lint": "eslint --fix",
    "format": "prettier --write",
    "clean-install": "rimraf pnpm-lock.yaml && rimraf node_modules && pnpm install",
    "clean-e2e": "rimraf ./cypress/downloads && rimraf ./cypress/screenshots && rimraf --glob ./cypress/e2e-results.*.xml",
    "stop-only": "node stop-only.js"
  },
  "dependencies": {
    "@braintree/sanitize-url": "7.0.2",
    "@primevue/themes": "4.0.3",
    "@sass-collective/strip-unit": "3.1.2",
    "@vee-validate/yup": "4.13.2",
    "abortcontroller-polyfill": "1.7.5",
    "aws-amplify": "6.5.3",
    "axios": "1.7.4",
    "chart.js": "4.4.3",
    "copyfiles": "2.4.1",
    "core-js": "3.37.1",
    "d3": "7.9.0",
    "dotenv": "16.4.5",
    "glob": "11.0.0",
    "google-palette": "1.1.1",
    "ip": "2.0.1",
    "json5": "2.2.3",
    "lodash-es": "4.17.21",
    "pinia": "2.1.7",
    "primeicons": "7.0.0",
    "primevue": "4.0.3",
    "qr-code-styling": "1.6.0-rc.1",
    "sass": "1.77.4",
    "semver": "7.6.2",
    "svg-pan-zoom": "3.6.1",
    "sweetalert2": "11.14.1",
    "tslib": "2.6.2",
    "typescript-logging": "2.2.0",
    "typescript-logging-log4ts-style": "2.2.0",
    "uuid": "9.0.1",
    "vee-validate": "4.13.0",
    "vue": "3.4.27",
    "vue-json-pretty": "2.4.0",
    "vue-router": "4.3.2",
    "vue-showdown": "4.2.0",
    "vue3-clipboard": "1.0.0",
    "vuedraggable": "2.24.3",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^21.0.3",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@faker-js/faker": "8.4.1",
    "@mswjs/data": "0.16.1",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@pinia/testing": "0.1.3",
    "@testing-library/vue": "8.1.0",
    "@types/d3": "7.4.3",
    "@types/jsonpath": "0.2.4",
    "@types/lodash-es": "4.17.12",
    "@types/node": "18.17.18",
    "@types/uuid": "9.0.4",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "@vitejs/plugin-vue": "5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/compiler-dom": "3.4.27",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "cypress": "13.14.1",
    "eslint": "8.57.0",
    "eslint-plugin-cypress": "3.3.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.26.0",
    "functions-have-names": "1.2.3",
    "happy-dom": "12.10.3",
    "headers-polyfill": "4.0.3",
    "msw": "2.3.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.40",
    "prettier": "3.3.0",
    "prettier-plugin-tailwindcss": "0.6.5",
    "rimraf": "5.0.7",
    "start-server-and-test": "2.0.3",
    "tailwindcss": "3.4.7",
    "tailwindcss-primeui": "0.3.4",
    "typescript": "5.2.2",
    "vite": "5.2.12",
    "vite-plugin-dts": "3.9.1",
    "vitest": "1.6.0",
    "vue-tsc": "2.0.19"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "types": "./types",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@badeball/cypress-cucumber-preprocessor",
      "core-js",
      "cypress",
      "esbuild",
      "msw",
      "vue-demi"
    ],
    "onlyBuiltDependencies": [
      "@badeball/cypress-cucumber-preprocessor",
      "core-js",
