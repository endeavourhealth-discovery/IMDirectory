    "@types/d3": "7.4.0",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "4.14.198",
    "@types/node": "18.17.18",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "6.7.2",
