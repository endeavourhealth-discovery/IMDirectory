  "scripts": {
    "dev": "run-p swagger-autogen tsc:dev vite:dev",
    "tsc:dev": "tsc --noEmit --watch",
    "tsc:build": "tsc --noEmit",
    "vite:dev": "vite",
    "debug": "run-p debug:build debug:run",
    "debug:build": "vite build --sourcemap --watch",
