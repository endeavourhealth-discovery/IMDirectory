{
  "name": "im-library",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clear": "rimraf dist/*",
    "build": "npm run clear && vite build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --fix",
    "format": "prettier --write",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.0",
    "google-palette": "1.1.0",
    "typescript": "4.7.4",
    "vitest": "0.15.1"
  },
  "dependencies": {
    "@types/d3": "7.4.0",
    "d3": "7.7.0",
    "lodash": "4.17.21",
    "rimraf": "3.0.2",
    "uuid-random": "1.3.2",
    "vite": "3.2.5",
    "vite-plugin-dts": "1.7.1"
  },
  "main": "./dist/packages/im-library/src/im-library.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "module": "dist/packages/im-library/src",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/packages/im-library/src/im-library.js",
      "require": "./dist/packages/im-library/src/im-library.cjs"
    },
    "./config/": {
      "types": "./dist/types/config/index.d.ts",
      "import": "./dist/packages/im-library/src/config/",
      "require": "./dist/packages/im-library/src/config/"
    },
    "./config": {
      "types": "./dist/types/config/index.d.ts",
      "import": "./dist/packages/im-library/src/config/config.js",
      "require": "./dist/packages/im-library/src/config/config.cjs"
    },
    "./constants/": {
      "types": "./dist/types/constants/index.d.ts",
      "import": "./dist/packages/im-library/src/constants/modules/",
      "require": "./dist/packages/im-library/src/constants/modules/"
    },
    "./constants": {
      "types": "./dist/types/constants/index.d.ts",
      "import": "./dist/packages/im-library/src/constants/constants.js",
      "require": "./dist/packages/im-library/src/constants/constants.cjs"
    },
    "./enums/": {
      "types": "./dist/types/enums/index.d.ts",
      "import": "./dist/packages/im-library/src/enums/modules/",
      "require": "./dist/packages/im-library/src/enums/modules/"
    },
    "./enums": {
      "types": "./dist/types/enums/index.d.ts",
      "import": "./dist/packages/im-library/src/enums/enums.js",
      "require": "./dist/packages/im-library/src/enums/enums.cjs"
    },
    "./helpers/": {
      "types": "./dist/types/helpers/index.d.ts",
      "import": "./dist/packages/im-library/src/helpers/modules/",
      "require": "./dist/packages/im-library/src/helpers/modules/"
    },
    "./helpers": {
      "types": "./dist/types/helpers/index.d.ts",
      "import": "./dist/packages/im-library/src/helpers/helpers.js",
      "require": "./dist/packages/im-library/src/helpers/helpers.cjs"
    },
    "./interfaces": "./dist/types/interfaces/index.d.ts",
    "./models/": {
      "types": "./dist/types/models/index.d.ts",
      "import": "./dist/packages/im-library/src/models/modules/"
    },
    "./models": {
      "types": "./dist/types/models/index.d.ts",
      "import": "./dist/packages/im-library/src/models/models.js",
      "require": "./dist/packages/im-library/src/models/models.cjs"
    },
    "./vocabulary/": {
      "types": "./dist/types/vocabulary/index.d.ts",
      "import": "./dist/packages/im-library/src/vocabulary/modules/",
      "require": "./dist/packages/im-library/src/vocabulary/modules/"
    },
    "./vocabulary": {
      "types": "./dist/types/vocabulary/index.d.ts",
      "import": "./dist/packages/im-library/src/vocabulary/vocabulary.js",
      "require": "./dist/packages/im-library/src/vocabulary/vocabulary.cjs/"
    }
  },
  "typesVersions": {
    "*": {
      "config": [
        "dist/types/config/index.d.ts"
      ],
      "constants": [
        "dist/types/constants/index.d.ts"
      ],
      "enums": [
        "dist/types/enums/index.d.ts"
      ],
      "helpers": [
        "dist/types/helpers/index.d.ts"
      ],
      "models": [
        "dist/types/models/index.d.ts"
      ],
      "vocabulary": [
        "dist/types/vocabulary/index.d.ts"
      ]
    }
  }
}
