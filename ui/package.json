{
  "name": "im-ui",
  "version": "2.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm copy-themes && run-p tsc:dev dev:vite",
    "tsc:dev": "vue-tsc --noEmit --watch",
    "tsc:build": "vue-tsc --noEmit",
    "copy-themes": "copyfiles -u 3 ./node_modules/primevue/resources/themes/**/* ./public/",
    "dev:vite": "vite --port 8082",
    "dev:mocked": "vite --mode mock  --port 8082 ",
    "preview": "vite preview --port 8082",
    "build": "pnpm copy-themes && pnpm tsc:build && vite build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:run": "cypress run",
    "test:e2e": "cypress open --browser chrome",
    "test:e2e:ci": "start-server-and-test preview http://localhost:8082 cypress:run",
    "lint": "eslint --fix",
    "format": "prettier --write"
  },
  "dependencies": {
    "@braintree/sanitize-url": "7.0.2",
    "@sass-collective/strip-unit": "3.1.2",
    "abortcontroller-polyfill": "1.7.5",
    "aws-amplify": "6.3.4",
    "axios": "1.7.2",
    "chart.js": "4.4.3",
    "copyfiles": "2.4.1",
    "core-js": "3.37.1",
    "d3": "7.9.0",
    "dotenv": "16.4.5",
    "google-palette": "1.1.1",
    "ip": "2.0.1",
    "json5": "2.2.3",
    "lodash-es": "4.17.21",
    "path": "0.12.7",
    "pinia": "2.1.7",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primevue": "3.53.0",
    "qr-code-styling": "1.6.0-rc.1",
    "sass": "1.77.4",
    "semver": "7.6.2",
    "svg-pan-zoom": "3.6.1",
    "sweetalert2": "11.11.0",
    "tslib": "2.6.2",
    "uuid": "9.0.1",
    "vee-validate": "4.13.0",
    "vue": "3.4.27",
    "vue-json-pretty": "2.4.0",
    "vue-router": "4.3.2",
    "vue-showdown": "4.2.0",
    "vue3-clipboard": "1.0.0",
    "vuedraggable": "2.24.3",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@pinia/testing": "0.1.3",
    "@testing-library/vue": "8.1.0",
    "@types/d3": "7.4.3",
    "@types/jsonpath": "0.2.4",
    "@types/lodash-es": "4.17.12",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "@vitejs/plugin-vue": "5.0.5",
    "@vue/compiler-dom": "3.4.27",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.6",
    "cross-env": "7.0.3",
    "cypress": "13.10.0",
    "eslint": "8.57.0",
    "eslint-plugin-cypress": "3.3.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.26.0",
    "functions-have-names": "1.2.3",
    "headers-polyfill": "4.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.0",
    "start-server-and-test": "2.0.3",
    "vue-tsc": "2.0.19"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "types": "./types"
}
